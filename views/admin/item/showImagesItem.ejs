<div
  class="tab-pane fade show active"
  id="show-images-item"
  role="tabpanel"
  aria-labelledby="show-images-item-tab"
>
<div class="row">
  <% for(let i = 0; i<items.imageIds.length; i++){ %>
    <div class="col-md-4 col-sm-6-col-12 mb-3">
      <div class="card shadow mb-4">
        <div class="card-body">
          <img src="/<%= items.imageIds[i].imageUrl %>" alt="item" class="w-100">
        </div>
      </div>
    </div>
  
  <% } %>  
  
</div>
<a href="/admin/item" class="btn btn-sm btn-info">&laquo; back</a>
</div>
